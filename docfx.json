{
  "metadata": [
    {
      "src": [
        {
          "src": "UnityCsReference/Projects/CSharp/",
          "files": [ "*.csproj" ]
        }
      ],
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "netstandard2.1"
      },
      "dest": "Temp/ScriptReference"
    }
  ],
  "build":
  {
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "content": [
      {
        "src": "Temp/ScriptReference",
        "files": [ "*.yml" ],
        "dest": "ScriptReference"
      }
    ],
    "dest": "UnityCsReference/_site"
  }
}